fn fibonacci(n: int) -> int {
    if n <= 1 {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

// Ø¯Ø§Ù„Ø© Ø­Ø³Ø§Ø¨ Ù…Ø¬Ù…ÙˆØ¹ Ø£ÙˆÙ„ n Ø£Ø±Ù‚Ø§Ù… ÙÙŠØ¨ÙˆÙ†Ø§ØªØ´ÙŠ
fn fibonacci_sum(n: int) -> int {
    let sum = 0;
    let i = 0;
    while i < n {
        sum = sum + fibonacci(i);
        i = i + 1;
    }
    return sum;
}

