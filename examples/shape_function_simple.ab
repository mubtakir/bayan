fn calculate_general_function(a0: float, a1: float, a2: float, a3: float, x: float, y: float) -> float {
    // Ù…Ø¹Ø§Ø¯Ù„Ø© Ø¹Ø§Ù…Ø©: a0 + a1*x + a2*y + a3*x*y
