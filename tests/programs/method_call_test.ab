// Method call test (Expert recommendation: Priority 1 - Phase 3)

// Define a struct
struct Person {
    name: string;
    age: int;
}

// Implement inherent methods for the struct
impl Person {
    fn new(name: string, age: int) -> Person {
        return Person { name: name, age: age };
    }

    fn get_age(person: Person) -> int {
        return person.age;
    }
}

fn main() -> int {
    let person = Person { name: "Ahmed", age: 25 };
    let age = person.get_age();
    return age;
}
