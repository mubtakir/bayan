// اختبار أساسي للسمات - الأولوية القصوى للخبير

trait Display {
    fn to_string(self) -> string;
}

struct Dog {
    name: string,
}

impl Display for Dog {
    fn to_string(self) -> string {
        return "Dog: " + self.name;
    }
}

fn main() {
    let dog = Dog { name: "بوبي" };
    let result = dog.to_string();
}
